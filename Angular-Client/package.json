{
  "name": "angular2-quickstart",
  "version": "1.0.0",
  "scripts": {
    "start": "tsc && concurrently \"npm run tsc:w\" \"npm run lite\" ",
    "lite": "lite-server",
    "liteProd": "lite-server --config=config/lite-prod-config.json",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "test": "tsc && concurrently \"tsc -w\" \"karma start karma.conf.js\"",
    "lint": "tslint ./app/**/*.ts -t verbose",
    "buildProd": "gulp build:web",
    "startProd": "gulp build:web && npm run liteProd",
    "buildAll": "gulp build:all"
  },
  "license": "ISC",
