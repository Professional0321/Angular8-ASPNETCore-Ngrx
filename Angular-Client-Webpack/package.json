{
  "name": "angular2-quickstart",
  "version": "1.0.0",
  "scripts": {
    "start": "tsc && webpack-dev-server --hot --open",
    "lite": "lite-server --config=config/lite-prod-config.json",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "test": "tsc && concurrently \"tsc -w\" \"karma start karma.conf.js\"",
    "lint": "tslint ./app/**/*.ts -t verbose",
    "ngc": "ngc -p tsconfig-aot.json --locale=en-US",
    "webpack-dev": "set NODE_ENV=development && webpack",
    "webpack-prod": "npm run ngc && set NODE_ENV=production && webpack",
    "buildAll": "npm run webpack-dev && npm run webpack-prod && gulp build:all"
  },
  "license": "ISC",
  "dependencies": {
    "@angular/common": "4.0.0-rc.2",
