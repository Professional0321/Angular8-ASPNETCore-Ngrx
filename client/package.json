{
  "name": "emeal",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve -o",
    "test": "ng test",
    "test-ci": "ng test --browsers PhantomJS --single-run",
    "lint": "./node_modules/.bin/tslint -c tslint.json -p tsconfig.json",
    "e2e": "ng e2e",
    "build": "ng build --prod --stats-json --output-path=\".dist/web\"",
    "build-web": "npm run build",
    "prepare-desktop": "mkdirp ./.temp/electron && ncp assets/electron .temp/electron/ && ncp .dist/web .temp/electron/ && rimraf .dist/desktop/",
    "build-desktop": "npm run prepare-desktop && electron-packager .temp/electron/ --electronVersion=2.0.0 --icon=\"assets/electron/icon\" --platform=win32,linux --out=./.dist/desktop/",
    "build-mobile": "npm run mobile-prepare && npm run mobile-prepare-windows && npm run mobile-prepare-android && npm run mobile-generate-assets && npm run mobile-build && npm run copy-mobile-to-dist",
    "mobile-prepare": "mkdirp ./.temp/mobile/www && ncp assets/cordova .temp/mobile/ && ncp .dist/web .temp/mobile/www && rimraf .dist/mobile/",
    "mobile-prepare-android": "mkdirp .dist/mobile/android && cd .temp/mobile && cordova prepare android && cd ../..",
    "mobile-prepare-windows": "mkdirp .dist/mobile/windows && cd .temp/mobile && cordova prepare windows",
    "mobile-generate-assets": "cd .temp/mobile && cordova-icon --icon=\"../../assets/img/icon.png\" && cordova-splash --splash=\"../../assets/img/splash.png\"",
    "mobile-build": "cd .temp/mobile && cordova build && cd ../..",
    "copy-mobile-to-dist": "ncp .temp/mobile/platforms/android .dist/mobile/android && ncp .temp/mobile/platforms/windows .dist/mobile/windows",
    "build-all": "rimraf .dist && npm run build-web && npm run build-mobile && npm run build-desktop",
    "bundle-report": "webpack-bundle-analyzer .dist/web/stats.json",
    "cleanup": "rimraf .temp"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^6.0.0",
    "@angular/common": "^6.0.0",
    "@angular/compiler": "^6.0.0",
    "@angular/core": "^6.0.0",
    "@angular/forms": "^6.0.0",
    "@angular/http": "^6.0.0",
    "@angular/platform-browser": "^6.0.0",
    "@angular/platform-browser-dynamic": "^6.0.0",
    "@angular/router": "^6.0.0",
    "@angular/service-worker": "^6.0.0",
    "@aspnet/signalr": "^1.0.0-rc1-update1",
    "@ngrx/effects": "^5.2.0",
    "@ngrx/router-store": "^5.2.0",
    "@ngrx/store": "^5.2.0",
    "angular2-toaster": "^5.0.0",
    "bootstrap": "^3.3.7",
    "core-js": "^2.5.4",
    "electron": "^1.8.2",
    "font-awesome": "^4.7.0",
    "jquery": "^3.3.1",
    "ngx-electron": "^1.0.4",
    "rxjs": "^6.0.0",
    "rxjs-compat": "^6.0.0-uber-rc.5",
    "rxjs-tslint": "^0.1.3",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular/compiler-cli": "^6.0.0",
    "@angular-devkit/build-angular": "~0.6.0",
    "typescript": "~2.7.2",
    "@angular/cli": "~6.0.0",
    "@angular/language-service": "^6.0.0",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.2.1",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~1.7.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~1.4.2",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.3.0",
    "ts-node": "~5.0.1",
    "tslint": "~5.9.1",
    "cordova-icon": "^0.11.0",
    "cordova-splash": "^0.10.0",
    "electron-packager": "^11.1.0",
    "karma-cli": "~1.0.1",
    "karma-mocha-reporter": "^2.2.5",
    "karma-phantomjs-launcher": "^1.0.4",
    "mkdirp": "^0.5.1",
    "ncp": "^2.0.0",
    "shelljs": "^0.8.1",
    "sw-precache": "^5.2.1",
    "webpack-bundle-analyzer": "^2.11.1"
  }
}
